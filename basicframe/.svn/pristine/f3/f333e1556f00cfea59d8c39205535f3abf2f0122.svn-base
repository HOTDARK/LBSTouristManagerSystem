package com.hd.sfw.core.utils;import java.math.BigInteger;import java.security.MessageDigest;import java.security.NoSuchAlgorithmException;/** *  * Md5密钥处理工具类 * */public class Md5Utils {		/**	 * 加密MD5，返回全小写密文	 * @param s：全小写密文	 * @return	 */	public static String md5(String s) {		try {			MessageDigest m = MessageDigest.getInstance("MD5");			m.update(s.getBytes(), 0, s.length());			BigInteger i = new BigInteger(1, m.digest());			return String.format("%1$032x", i);		} catch (NoSuchAlgorithmException e) {			e.printStackTrace();		}		return null;	}		/**	 * 加密MD5，返回全大写密文	 * @param s：全大写密文	 * @return	 */	public static String md5Upper(String s) {		return md5(s)==null?null:md5(s).toUpperCase();	}}