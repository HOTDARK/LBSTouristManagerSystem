package com.hd.sfw.webservice.exception;

/**
 * webservice 异常,应当显示的处理此异常
 * @version	0.0.1
 * @author	<a href="mailto:sunjian@iwangding.com">孙剑</a>
 * @date	2014-9-10 上午11:07:27
 */
public class IwdWebServiceException extends Exception {
	private static final long serialVersionUID = 1L;

	public IwdWebServiceException(String msg) {
		super(msg);
	}
	
	public IwdWebServiceException(String msg,Throwable throwable){
		super(msg,throwable);
	}
	
	public IwdWebServiceException(Throwable throwable){
		super(throwable);
	}
	
	public IwdWebServiceException() {
	}
}
