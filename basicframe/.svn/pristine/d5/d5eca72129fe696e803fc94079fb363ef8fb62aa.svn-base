public enum AuthType {
	/**
	 * 不需要授权
	 */
	NONE,
	/**
	 * soap协议头方式,默认格式为
	 * <pre>
	 * &lt;AuthenticationToken&gt;
     * 	&lt;Username&gt;admin&lt;/Username&gt;
     * 	&lt;Password&gt;admin&lt;/Password&gt;
     * &lt;/AuthenticationToken&gt;
	 * </pre>
	 *    
	 */
	SOAP_HEADER,
	/**
	 * http POST请求
	 */
	POST
}
