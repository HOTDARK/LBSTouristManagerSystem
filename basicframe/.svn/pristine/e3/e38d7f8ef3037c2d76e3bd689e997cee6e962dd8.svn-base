package com.hd.sys.core.properties.typehandler;

import com.hd.sys.core.properties.TypeHandler;

/**
 * 整形数据处理
 * @author sunjian
 * @version V1.0, 2013-5-16 下午02:33:39
 */
public class IntegerTypeHandler implements TypeHandler {

	@Override
	public Object toObj(String str) {
		return Integer.parseInt(str);
	}

	@Override
	public String toStr(Object obj) {
		return obj.toString();
	}

}
