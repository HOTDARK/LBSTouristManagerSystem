package com.hd.workflow.engine.pvm;

/**
 * 分支上附属条件判断的结果
 * @version	0.0.1
 * @author	<a href="mailto:sunjian@iwangding.com">孙剑</a>
 * @date	2015-2-12 上午11:16:07
 */
public class ConditionResult {
	
	private boolean checked;
	
	private PvmCondition condition;
	
	private PvmTransition pvmTransition;

	/**
	 * @return the checked
	 */
	public boolean isChecked() {
		return checked;
	}

	/**
	 * @param checked the checked to set
	 */
	public void setChecked(boolean checked) {
		this.checked = checked;
	}

	/**
	 * @return the condition
	 */
	public PvmCondition getCondition() {
		return condition;
	}

	/**
	 * @param condition the condition to set
	 */
	public void setCondition(PvmCondition condition) {
		this.condition = condition;
	}

	/**
	 * @return the pvmTransition
	 */
	public PvmTransition getPvmTransition() {
		return pvmTransition;
	}

	/**
	 * @param pvmTransition the pvmTransition to set
	 */
	public void setPvmTransition(PvmTransition pvmTransition) {
		this.pvmTransition = pvmTransition;
	}
	
}
