.guresule-type
{
	padding:2px  5px;
}
.guresule-type button
{
	line-height: 32px;
	background: #FFFFFF;
	border-radius: 0;
	border: 1px solid #EAEAEA;
}
.guresule-type .active
{
	background-color: #EAEAEA;
	box-shadow: none !important;
}
.guidetype-icon
{
	display: inline-block;
	width: 32px;
	height: 32px;
	float: left;
}
.icon-691
{
	background: url(../images/guide/691.png);
}
.icon-678
{
	background: url(../images/guide/678.png);
}
.icon-offline
{
	background: url(../images/guide/offline.png);
}
.icon-slow
{
	background: url(../images/guide/slow.png);
}
.handle
{
	padding-left: 30px;
} 
.handle .control-btn
{
	border: 1px solid #f0edff;
	color: #4d4d4d;
	padding: 5px 15px;
	background-color: #f9f9f9;
	text-align: center;
	word-break: keep-all;
	white-space: nowrap;
	
}

 .resnum
{
  padding:2px  6px !important;
  text-align: center;
  margin-right: 10px;
  border-radius: 50%;
  color: #ffffff !important;
  background-color: #fa474a;
}

#guideBox .disable a {
	text-decoration:none;
	
}
