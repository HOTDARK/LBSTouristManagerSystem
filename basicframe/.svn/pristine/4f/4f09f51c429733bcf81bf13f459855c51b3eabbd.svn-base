.search-box
{
	padding: 10px;
	border-bottom: 1px solid #fafafa;
}
.fixcontent
{
	position: relative;
	margin-right: 310px;
   _height:expression((document.documentElement.clientHeight||document.body.clientHeight)>500?"500px":"");}
	
}
.wf-iframe
{
	float: left; 
	width: 450px; 
	height:500px;
	/*border: 1px solid #eaeaea;*/
	margin: 5px;
}
.in_title
{
	padding-top: 5px;
	color: #ff6600;
	line-height: 24px;
	
	
}
.flowicon
{
	display: inline-block;
	float: left;
	width: 24px;
	height: 24px;
	background: url(../images/guide/flow.png);
}
.fixright
{
	float: right; overflow: hidden;
	width: 300px;
	
}
.fixright .panel
{
	border: 1px solid #eee;
	margin: 10px 5px;
}
.fixright .panel .panel-heading
{
	background-color: #fafafa;
	border-bottom: 1px solid #EAEAEA;
	color: #3497db;
	font-size: 14px;
}
.fixright .panel .panel-heading span
{
	
	border-left: 4px solid #3497db;
	padding-left: 5px;
}
.fixright ul
{
	padding: 0;
	margin: 0;
}

.fixright li
{
	list-style: none;
	padding: 7px 0;
	
	
}
.fixright .title
{
	color: #666666;
	
}
.fixright li span.info
{
	font-size: 14px;
}
.fixright li span
{
	color: #000;
	padding-left: 5px;
}
.fixright h5
{
	font-size: 14px;
	font-weight: normal;
}
.fixright .desc
{
	
	
	padding: 5px;
}
.fixright .desc span
{
	font-size: 14px;
	padding-left: 20px;
	background-color: #fafafa;
	line-height: 30px;
	background: url(../images/guide/desc.png) no-repeat left 0px;	
}
.guidecontent
{
	position: relative;	
}
.guidecontent ul
{
	padding: 0;
	margin: 0;
}
.guidecontent li
{
	list-style: none;
	position: relative;	
}
.guidecontent .stitle
{
	position: relative;	
}
.guidecontent .time-line {
	position: absolute;
	top: 16px;
	bottom: 0px;
	left: 467px;
	width: 1px;
	border-left: #eee solid 2px;
	z-index: 0;
	
}
.guide-step  a{
	padding:8px 10px;
	cursor: pointer;
}
.guide-step  a:hover
{
	text-decoration: none;
}
.guide-step  a.select
{
	color: #ff6600;
	cursor: default;
	
}
.per-icon {	
	display: inline-block;
	width: 16px;
	height: 16px;
	margin-right: 10px;	
	float: left;
	background:url(../images/point.png) no-repeat left 0px;
}
.current
{
	/*border: 2px solid #ff6600;*/
}
