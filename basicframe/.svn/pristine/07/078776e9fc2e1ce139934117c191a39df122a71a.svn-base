<div class="modal" ng-controller="WorkflowConclusionPopupCtrl">
    <div class="modal-dialog">
        <div class="modal-content">
			<div class="modal-header">
			    <button type="button" class="close" data-dismiss="modal" aria-hidden="true" ng-click="close()">&times;</button>
			    <h2>编辑总结论 </h2>
			</div>
			<div class="modal-body">
			    <div class="detail-group clearfix">
                    <div class="col-xs-12">
						<div class="form-group">
					   		<label for="codeField">编码</label>
					   		<input type="text" id="codeField" class="form-control" ng-model="conclusion.code" placeholder="编码" />
						</div>
						<div class="form-group">
					   		<label for="resultField">结果</label>
					   		<input type="text" id="resultField" class="form-control" ng-model="conclusion.result" placeholder="结果" />
						</div>
						<div class="form-group">
					   		<label for="stateField">提示状态</label>
					   		<select id="stateField" class="form-control" ng-model="conclusion.state">
					   			<option value="normal">正常</option>
					   			<option value="error">异常</option>
					   			<option value="warn">警告</option>
					   		</select>
						</div>
						<div class="form-group">
					   		<label for="tipsField">处理建议</label>
					   		<textarea id="tipsField" class="form-control" ng-model="conclusion.tips" style="width:100%; height:100%;"></textarea>
						</div>
			        </div>
			    </div>
			</div>
			<div class="modal-footer">
			    <button ng-disabled="state.subprocessError" ng-click="clear()" class="btn btn-primary">清除</button>
			    <button ng-disabled="state.subprocessError" ng-click="save()" class="btn btn-primary" translate>ACTION.SAVE</button>
			</div>
		</div>
	</div>
</div>
